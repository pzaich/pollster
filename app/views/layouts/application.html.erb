<!DOCTYPE html>
<html>
<head>
  <title>Pollster</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% flash.each do |key, message| %>
  <%= content_tag :p, message, :class => "alert #{key}" %>
<% end %>	

<%= yield %>

</body>
</html>
